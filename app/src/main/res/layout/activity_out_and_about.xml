<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.stellajovanovic.hotelapplication.OutAndABout.OutAndAboutActivity">

    <WebView xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:id="@+id/webView1"
             android:layout_width="365dp"
             android:layout_height="441dp"
             android:layout_alignParentBottom="true"
             android:layout_alignParentStart="true"
             tools:context=".BrowserActivity"
             tools:layout_editor_absoluteY="62dp"
             tools:layout_editor_absoluteX="11dp"></WebView>

    <Button
        android:id="@+id/button2"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorPrimary"
        android:text="Navigate to Trip Advisor"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/webView1"/>


</android.support.constraint.ConstraintLayout>
